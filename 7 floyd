#include <iostream>
#include <vector>
#include <iomanip>
using namespace std;

const int INF = 1e9;

void floydWarshall(vector<vector<int>>& g, int n) {
    vector<vector<int>> d = g;
    for (int k = 0; k < n; ++k)
        for (int i = 0; i < n; ++i)
            for (int j = 0; j < n; ++j)
                if (d[i][k] < INF && d[k][j] < INF)
                    d[i][j] = min(d[i][j], d[i][k] + d[k][j]);

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j)
            cout << setw(7) << (d[i][j] == INF ? "INF" : to_string(d[i][j]));
        cout << '\n';
    }
}

int main() {
    int n = 4;
    vector<vector<int>> graph = {
        {0,   3, INF, 5},
        {2,   0, INF, 4},
        {INF, 1,   0, INF},
        {INF, INF, 2, 0}
    };
    floydWarshall(graph, n);
    return 0;
}
