class Employee:
    def __init__(self, emp_id, name, dept, salary):
        self.emp_id = emp_id
        self.name = name
        self.dept = dept
        self.salary = salary

    def __str__(self):
        return f"{self.emp_id} - {self.name} - {self.dept} - {self.salary}"


def bubble_sort(emp_list):
    n = len(emp_list)
    for i in range(n - 1):
        for j in range(n - i - 1):
            if emp_list[j].emp_id > emp_list[j + 1].emp_id:
                emp_list[j], emp_list[j + 1] = emp_list[j + 1], emp_list[j]


def insertion_sort(emp_list):
    for i in range(1, len(emp_list)):
        key = emp_list[i]
        j = i - 1
        while j >= 0 and emp_list[j].name > key.name:
            emp_list[j + 1] = emp_list[j]
            j -= 1
        emp_list[j + 1] = key


employees = []

n = int(input("Enter number of employees: "))

for i in range(n):
    print(f"\nEnter details of employee {i+1}:")
    emp_id = int(input("Employee ID: "))
    name = input("Name: ")
    dept = input("Department: ")
    salary = float(input("Salary: "))

    employees.append(Employee(emp_id, name, dept, salary))


print("\nOriginal List of Employees:")
for e in employees:
    print(e)


bubble_sort(employees)
print("\nSorted by Employee ID (Bubble Sort):")
for e in employees:
    print(e)


insertion_sort(employees)
print("\nSorted by Name (Insertion Sort):")
for e in employees:
    print(e)


print("\nTime & Space Complexity:")
print("Bubble Sort -> Time: O(n^2), Space: O(1)")
print("Insertion Sort -> Time: O(n^2) Avg/Worst, O(n) Best, Space: O(1)")
